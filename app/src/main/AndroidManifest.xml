<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moviemaster">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MovieMaster">
        <activity android:name=".Activity.MovieDetails"></activity>
        <activity android:name=".Activity.EditMovie" />
        <activity android:name=".Activity.Ratings" />
        <activity android:name=".Activity.SearchMovie" />
        <activity android:name=".Activity.EditMovieList" />
        <activity android:name=".Activity.Favourites" />
        <activity android:name=".Activity.DisplayMovies" />
        <activity android:name=".Activity.Register_Movie" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>